#include ".Xresources.d/base16-default.dark.256.xresources"
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
!Fake transparency
!urxvt*transparent: true
!urxvt*shading: 30
!True transparency
!urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800
URxvt.letterSpace: -1
!URxvt.font: xft:Meslo LG M for Powerline:style=RegularForPowerline:size=11
!URxvt.font: xft:Roboto Mono for Powerline:style=Regular:size=11
URxvt.font: xft:Inconsolata\\-g for Powerline:style=g:size=11
URxvt.scrollBar:false
